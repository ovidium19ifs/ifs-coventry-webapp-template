    "less-middleware": "^2.2.1",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.11"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
